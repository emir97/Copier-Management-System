@model iCopy.Model.Response.Company
@{
    Layout = null;
}
<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLabel">@_sharedLocalizer.Details</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
</div>
<div class="modal-body">
    <ul class="nav nav-tabs  nav-tabs-line nav-tabs-line-brand" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" data-toggle="tab" href="#company_details" role="tab"><i class="flaticon-time-2"></i> @_sharedLocalizer.CompanyDetails</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" data-toggle="tab" href="#account_details" role="tab"><i class="flaticon-multimedia"></i> @_sharedLocalizer.AccountDetails</a>
        </li>
    </ul>
    <div class="tab-content">
        <div class="tab-pane active" id="company_details" role="tabpanel">
            <div class="row">
                <div class="col-md-4">
                    <img src="@Model.ProfilePhoto.Path"/>
                </div>
            </div>
        </div>
        <div class="tab-pane" id="kt_tabs_9_3" role="tabpanel">
            Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. 
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">@_sharedLocalizer.Close</button>
</div>
